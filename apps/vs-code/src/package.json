{
  "name": "ng-studio",
  "displayName": "Angular Studio",
  "version": "0.0.0",
  "main": "main.js",
  "license": "MIT",
  "engines": {
    "vscode": "^1.44.0"
  },
  "keywords": [],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ngStudio.start",
    "onView:moduleTree"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ng-tree",
          "title": "Angular Tree",
          "icon": "assets/copy.svg"
        }
      ]
    },
    "views": {
      "ng-tree": [
        {
          "id": "moduleTree",
          "name": "Module Tree"
        },
        {
          "id": "templateTree",
          "name": "Template Tree"
        }
      ]
    },
    "commands": [
      {
        "command": "ngStudio.start",
        "title": "Start"
      },
      {
        "command": "template.selected",
        "title": "Select template ast node"
      }
    ],
    "configuration": {
      "title": "Ng Studio"
    }
  }
}