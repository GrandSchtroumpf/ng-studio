{
  "name": "ng-studio",
  "displayName": "Angular Studio",
  "version": "0.0.0",
  "main": "main.js",
  "license": "MIT",
  "engines": {
    "vscode": "^1.46.0"
  },
  "keywords": [],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:workspaceTree"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ng-tree",
          "title": "Angular Tree",
          "icon": "assets/copy.svg"
        }
      ]
    },
    "views": {
      "ng-tree": [
        {
          "id": "workspaceTree",
          "name": "Workspace"
        },
        {
          "id": "templateTree",
          "name": "Template Tree"
        }
      ]
    },
    "commands": [
      {
        "command": "ngStudio.start",
        "title": "Start"
      },
      {
        "command": "template.selected",
        "title": "Select template ast node"
      },
      {
        "command": "template.add",
        "title": "Add a child",
        "icon": {
          "dark": "assets/dark/add.svg",
          "light": "assets/light/add.svg"
        }
      },
      {
        "command": "template.remove",
        "title": "Add a child",
        "icon": {
          "dark": "assets/dark/delete.svg",
          "light": "assets/light/delete.svg"
        }
      }
    ],
    "menus": {
      "view/item/context": [
        {
          "command": "template.add",
          "when": "viewItem == element",
          "group": "inline"
        },
        {
          "command": "template.remove",
          "when": "viewItem == element",
          "group": "inline"
        }
      ]
    },
    "configuration": {
      "title": "Ng Studio"
    }
  }
}